@model marble.ViewModels.ContactPageModel
@{
    ViewBag.Title = "Contact";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .header-font {
        font-family: 'Cairo', sans-serif;
        text-align: right;
        direction: rtl;
    }

    .footer-font {
        font-family: 'Cairo', sans-serif;
        text-align: right;
    }

    .header-font-bold {
        font-family: 'Almarai', sans-serif;
        text-align: right;
        direction: rtl;
    }

    .line_1 {
        font-family: 'Cairo', sans-serif;
        text-align: right;
        font-size: larger;
    }

    .line_2 {
        font-family: 'Cairo', sans-serif;
        text-align: right;
        font-weight: bolder;
        font-size: larger;
    }

    .rtl {
        direction: rtl;
        text-align: right;
    }

    .text-font {
        font-family: GE-Flow-Regular;
    }
</style>
<main>
    <div class="row rtl">
        <aside class="col-xs line_1">
            <h1>من نحن</h1>
            <div id="desktop-filter">
                <ul id="menu-corporate" class="accordion-content">
                    <li style="text-align:right" id="menu-item-9772" class="menu-item menu-item-type-post_type menu-item-object-page current-menu-item page_item page-item-4488 current_page_item menu-item-9772"><a href="https://localhost:44382/Contact/Contact/" aria-current="page"> تواصل معنا</a></li>
                    <li style="text-align:right" id="menu-item-10170" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-10170"><a href="https://localhost:44382/">الصفحة الرئيسية</a></li>
                    <li style="text-align:right" id="menu-item-10170" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-10170"><a href="https://localhost:44382/Product/">الوثائق</a></li>
                    <li style="text-align:right" id="menu-item-10170" class="menu-item menu-item-type-post_type menu-item-object-page menu-item-10170"><a href="https://localhost:44382/About/About/">من نحن</a></li>

                </ul>
            </div>
        </aside>

        <div class="col-xl">
            <div class="row">
                <div class="col-full">
                    <iframe width="100%" height="600" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?width=100%25&amp;height=600&amp;hl=en&amp;q=1%20Grafton%20Street,%20Dublin,%20Ireland+(My%20Business%20Name)&amp;t=&amp;z=14&amp;ie=UTF8&amp;iwloc=B&amp;output=embed"><a href="http://www.gps.ie/">truck gps</a></iframe>
                    <section class="call-to-action  header-font" >
                        <div class="row">
                            <div class="col-s mb-1">
                                <span class="cta-title">العنوان</span>
                                <p>@Model.ContactInfo.Country</p>
                            </div>
                            <div class="col-s mb-1">
                                <span class="cta-title">الايميل</span>
                                <p><a href="/cdn-cgi/l/email-protection" class="__cf_email__" data-cfemail="e78b8889838889a793828a8a82958a8695858b82c984888a">@Model.ContactInfo.Email</a></p>
                            </div>
                            <div class="col-s mb-1">
                                <span class="cta-title">اتصل بنا</span>
                                <p>@Model.ContactInfo.Phone</p>
                            </div>
                        </div>

                    </section>
                </div>
            </div>
        </div>
    </div>
</main>