                },
            });
        </script>
        <script>
            $.get("https://freegeoip.app/json/", function (response) {
                var country = response.country_code;
                if (country == 'TR') document.getElementById('langs').style.display = "inline-block";
            }, "jsonp");
            $.get("https://freegeoip.app/json/", function (response) {
                var country = response.country_code;
                if (country == 'TR') document.getElementById('menu-item-10167').style.display = "none";
                if (country == 'TR') document.getElementById('menu-item-11925').style.display = "inline-block";
            }, "jsonp");
            $.get("https://freegeoip.app/json/", function (response) {
                var country = response.country_code;
                if (country == 'US' || country == 'CA' || country == 'UK' || country == 'TR') document.getElementById(country).style.display = "block";
            }, "jsonp");
            $.get("https://freegeoip.app/json/", function (response) {
                var country = response.country_code;
                if (country !== 'US') document.getElementById('sliderglb').style.display = "block";
                if (country == 'US') document.getElementById('slideratl').style.display = "block";
            }, "jsonp");
        </script>
